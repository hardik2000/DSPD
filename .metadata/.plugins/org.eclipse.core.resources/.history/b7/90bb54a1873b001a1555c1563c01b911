#include <stdio.h>
#include <stdlib.h>
#define Itemtype int
struct Node
{
	Itemtype data;
	struct Node *next;

};

int main(void)
{

	struct Node *N1,*N2,*N3,*ptr;
	N1=(struct Node *)malloc(sizeof(struct Node));
	N2=(struct Node *)malloc(sizeof(struct Node));
	N3=(struct Node *)malloc(sizeof(struct Node));
	N1->data=10;
	N2->data=20;
	N3->data=30;
	N1->next=NULL;
	ptr=N1;
	printf("%d ",ptr->data);
	N2->next=ptr;
	ptr=N2;
	printf("%d ",ptr->data);
	N3->next=ptr;
	ptr=N3;
	printf("%d ",ptr->data);
	printf("%d ",&ptr);
	printf("%d ",&N1);
	printf("%d ",&N2);
	printf("%d ",&N3);
	printf("%d",sizeof(ptr));
	return 0;
}
